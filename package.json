{
  "name": "gitlink",
  "version": "0.1.1",
  "description": "Get online link of file(Github, GitLab, Bitbucket)",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qinezh/gitlink.git"
  },
  "keywords": [
    "git"
  ],
  "author": "qinezh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qinezh/gitlink/issues"
  },
  "homepage": "https://github.com/qinezh/gitlink#readme",
  "dependencies": {
    "@types/fs-extra": "^3.0.2",
    "@types/node": "^7.0.22",
    "fs-extra": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "typescript": "^2.3.3",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "ts-jest": "^20.0.4"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/test/*.(ts|js)"
    ]
  }
}
